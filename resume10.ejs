<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
  <head>
    <title>Sample Resume</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <link
      rel="stylesheet"
      type="text/css"
      href="http://yui.yahooapis.com/2.7.0/build/reset-fonts-grids/reset-fonts-grids.css"
      media="all"
    />
    <link rel="stylesheet" type="text/css" href="resume.css" media="all" />
    <style>
      @page {
        size: auto;
        margin: 5mm;
      }
      html {
        zoom: 0.83;
      }

      /* uncomment for check A4 size */

      @media print {
        .footer,
        #non-printable {
          display: none !important;
        }
        #printable {
          display: block;
        }
      }
      /* //-- yui-grids style overrides -- */
    </style>
  </head>
  <body style="font-family: Georgia; color: rgb(255, 253, 253)">
    <div id="doc2" class="yui-t7">
      <div
        id="inner"
        style="
          padding: 10px 60px;
          margin: 5px auto;
          background: #f5f5f5;
          border: solid rgb(241, 90, 90);
          border-width: 8px 0 2px 0;
        "
      >
        <!--// .yui-gf -->
        <div
          id="hd"
          style="
            margin: 2.5em 0 1em 0;
            padding-bottom: 1.5em;
            border-bottom: 1px solid #ccc;
          "
        >
          <div class="yui-gc">
            <div class="yui-u first">
              <h1
                style="
                  text-align: center;
                  color: crimson;
                  font-size: 40px;
                  text-transform: uppercase;
                  letter-spacing: 3px;
                "
              >
                <%= firstName %> <%= lastName %>
              </h1>
            </div>
            <div class="yui-u">
              <div class="contact-info" style="margin-top: 7px">
                <h3 style="font-size: 122%; line-height: 24px; color: #333">
                  <%= address %>
                </h3>
                <h3 style="font-size: 122%; line-height: 24px; color: #333">
                  <a style="color: #990003"><%=city%></a>
                </h3>
                <h3 style="font-size: 122%; line-height: 24px; color: #333">
                  <a style="color: #990003"><%=state %></a>
                </h3>

                <h3 style="font-size: 122%; line-height: 24px; color: #333">
                  <a style="color: #990003"><%=zipCode %></a>
                </h3>
                <h3 style="font-size: 122%; line-height: 24px; color: #333">
                  <a style="color: #990003"><%=email %></a><br />
                </h3>
                <h3 style="font-size: 122%; line-height: 24px; color: #333">
                  <a style="color: #990003"><%=phoneNo %></a>
                </h3>

                <% if (links && links.length>0) { %> <% for(var i=0; i <
                links.length; i++) { %>
                <h3 style="font-size: 122%; color: #333">
                  <a style="color: #990003"><%= links[i] %></a> <br />
                </h3>
                <% } %> <% } %>
              </div>
            </div>
          </div>
        </div>

        <!--// .yui-gf -->
        <% if (objectives && objectives.length>0) { %>

        <div
          class="yui-gf"
          style="
            width: 80.2%;
            margin-bottom: 1em;
            padding-bottom: 1em;
            border-bottom: 2px solid #ccc;
            padding-right: 11em;
          "
        >
          <div class="yui-u first">
            <h2 style="font-size: 152%; font-weight: bold; color: #333">
              Objectives
            </h2>
          </div>

          <div class="yui-u" style="padding-right: 7px">
            <div class="talent" style="float: left">
              <ol style="float: left">
                <% for( let i = 0; i < objectives.length; i++ ) { %>

                <li
                  style="
                    padding-left: 100px;
                    width: 550px;
                    text-align: justify;
                    color: black;
                    font-size: 16px;
                    padding-bottom: 10px;
                    line-height: 24px;
                  "
                >
                  <%=objectives[i]%>
                </li>
                <% } %>
              </ol>
            </div>
          </div>
        </div>
        <% } %>

        <!--// .yui-gf -->
        <% if (skills && skills.length>0) { %>

        <div
          class="yui-gf"
          style="
            width: 80.2%;
            margin-bottom: 1em;
            padding-bottom: 1em;
            border-bottom: 2px solid #ccc;
            padding-right: 11em;
          "
        >
          <div class="yui-u first">
            <h2 style="font-size: 152%; font-weight: bold; color: #333">
              Skills
            </h2>
          </div>

          <div class="yui-u">
            <div class="talent" style="width: 32%; float: left">
              <ul
                class="talent"
                style="width: 32%; padding-left: 108px; float: left"
              >
                <% for( let i = 0; i < skills.length; i++ ) { %>
                <li
                  class="skill"
                  style="
                    line-height: 24px;
                    width: 40em;
                    font-size: 16px;
                    list-style-type: disc;
                    color: black;
                  "
                >
                  <%=skills[i]%>
                </li>
                <% } %>
              </ul>
            </div>
          </div>
        </div>
        <% } %>

        <!--// .yui-gf -->
        <% if (professionalSummary) { %>
        <div
          class="yui-gf"
          style="
            margin-bottom: 1em;
            padding-bottom: 1em;
            border-bottom: 1px solid #ccc;
          "
        >
          <div class="yui-u first">
            <h2 style="font-size: 152%; font-weight: bold; color: #333">
              Summary
            </h2>
          </div>
          <div class="yui-u">
            <p
              class="enlarge"
              style="
                color: black;
                font-size: 100%;
                font-size: 16px;
                text-align: justify;
                padding-right: 1em;
                line-height: 24px;
                padding-left: 50px;
              "
            >
              <%=professionalSummary%>
            </p>
          </div>
        </div>
        <% } %>
        <!--// .yui-gf-->
        <% if (workHistory && workHistory.length>0) { %>
        <div
          class="yui-gf"
          style="margin-bottom: 1em; border-bottom: 1px solid #ccc"
        >
          <div class="yui-u first">
            <h2
              style="
                font-size: 152%;
                font-weight: bold;
                padding-left: 1px;
                color: #333;
              "
            >
              Experience
            </h2>
          </div>
          <div class="yui-u" style="width: 587px">
            <% for( let i = 0; i < workHistory.length; i++ ) { %>

            <div
              class="job"
              style="
                position: relative;
                margin-bottom: 1em;
                padding-bottom: 1em;
              "
            >
              <h2
                style="
                  font-size: 130%;
                  font-weight: bolder;
                  color: #333;
                  padding-bottom: 6px;
                  padding-left: 20px;
                "
              >
                <%=workHistory[i].employer%>
              </h2>

              <h2
                style="
                  font-size: 120%;
                  padding-left: 20px;
                  color: #333;
                  padding-bottom: 1px;
                "
              >
                <%=workHistory[i].city%> - <%=workHistory[i].state%> -
                <%=workHistory[i].country%>
              </h2>

              <h2 style="font-size: 120%; padding-left: 20px; color: #333">
                <%=workHistory[i].jobTitle%>
              </h2>
              <h4
                style="
                  font-size: 122%;
                  position: absolute;
                  top: 0.35em;
                  right: 0;

                  color: #333;
                  left: 450px;
                "
              >
                <%= new Date(workHistory[i].startDate).getMonth() + 1 +'/'+new
                Date(workHistory[i].startDate).getFullYear() %> &raquo;
              </h4>

              <% if (workHistory[i].currentlyWorkHere) { %>
              <h4
                style="
                  font-size: 122%;
                  position: absolute;
                  right: 0.5px;
                  color: #333;
                  padding-left: 0px;
                  top: 5.55px;
                "
              >
                Current
              </h4>
              <% } %> <% if (!workHistory[i].currentlyWorkHere) { %>
              <h4
                style="
                  font-size: 122%;
                  position: absolute;
                  right: 0.5px;
                  color: #333;
                  padding-left: 0px;
                  top: 5.55px;
                "
              >
                <%= new Date(workHistory[i].endDate).getMonth() + 1 +'/'+new
                Date(workHistory[i].endDate).getFullYear() %>
              </h4>
              <% } %>

              <h2
                style="
                  font-size: 130%;
                  color: #333;
                  padding-top: 10px;
                  padding-left: 20px;
                  padding-bottom: 10px;
                  font-style: italic;
                "
              >
                Responsibilities
              </h2>
              <% if (workHistory[i].responsibility &&
              workHistory[i].responsibility.length>0) { %> <% for( let j = 0; j
              < workHistory[i].responsibility.length; j++ ) { %>
              <ul style="padding-left: 40px">
                <li
                  style="
                    line-height: 24px;
                    list-style-type: disc;
                    font-size: 16px;
                    color: rgb(15, 15, 15);
                    text-align: justify;
                  "
                >
                  <%= workHistory[i].responsibility[j] %>
                </li>
              </ul>
              <% } %> <% } %>

              <h2
                style="
                  font-size: 130%;
                  font-style: italic;
                  color: rgb(0, 0, 0);
                  padding-left: 20px;
                  padding-top: 10px;
                  padding-bottom: 10px;
                "
              >
                Environment
              </h2>
              <% if (workHistory[i].environment &&
              workHistory[i].environment.length>0) { %>

              <ul style="padding-left: 50px">
                <% for( let j = 0; j < workHistory[i].environment.length; j++ )
                { %>
                <li
                  style="
                    line-height: 24px;
                    list-style-type: disc;
                    color: rgb(0, 0, 0);
                    font-size: 16px;
                  "
                >
                  <%= workHistory[i].environment[j] %>
                </li>
                <% } %>
              </ul>
              <% } %>
            </div>
            <% } %>
          </div>
        </div>
        <% } %>

        <!--// .yui-gf -->

        <!--// .yui-gf -->
        <% if (education && education.length>0) { %>

        <div
          class="yui-gf"
          style="margin-bottom: 1em; border-bottom: 1px solid #ccc"
        >
          <div class="yui-u first">
            <h2 style="font-size: 152%; font-weight: bold; color: #333">
              Education
            </h2>
          </div>
          <div class="yui-u" style="padding-right: 12px">
            <% for( let i = 0; i < education.length; i++ ) { %>

            <div
              class="job"
              style="position: relative; padding-left: 47px; margin-bottom: 1em"
            >
              <h2 style="font-size: 142%; color: #333">
                <%= education[i].fieldOfStudy %> <%=education[i].degree%>
              </h2>
              <h3 style="font-size: 122%; color: #333">
                <%=education[i].schoolName%>,<%=education[i].schoolLocation%>,<%=education[i].state%>
              </h3>
              <h4 style="font-size: 122%; color: #333">
                <%= education[i].graduationYear %>
              </h4>
            </div>
            <% } %>
          </div>
        </div>
        <% } %> <% if (affiliations && affiliations.length>0) { %>

        <div
          class="yui-gf"
          style="
            margin-bottom: 1em;
            padding-bottom: 1em;
            border-bottom: 1px solid #ccc;
          "
        >
          <div class="yui-u first">
            <h2
              style="
                font-size: 152%;
                width: 587px;
                font-weight: bold;
                color: #333;
              "
            >
              Affiliations
            </h2>
          </div>

          <div class="yui-u">
            <div class="talent" style="float: left">
              <ul class="talent" style="padding-left: 50px; float: left">
                <% for( let i = 0; i < affiliations.length; i++ ) { %>
                <li
                  class="skill"
                  style="
                    line-height: 24px;
                    font-size: 16px;

                    list-style-type: disc;
                    color: black;
                  "
                >
                  <%=affiliations[i]%>
                </li>
                <% } %>
              </ul>
            </div>
          </div>
        </div>
        <% } %>
        <!--// .yui-gf -->

        <!--// .yui-gf -->

        <% if (certifications && certifications.length>0) { %>

        <div
          class="yui-gf"
          style="
            margin-bottom: 1em;
            padding-bottom: 1em;
            border-bottom: 2px solid #ccc;
          "
        >
          <div class="yui-u first">
            <h2 style="font-size: 152%; font-weight: bold; color: #333">
              Certifications
            </h2>
          </div>

          <div class="yui-u">
            <div class="talent" style="float: left">
              <ul class="talent" style="padding-left: 50px; float: left">
                <% for(var i=0; i < certifications.length; i++) { %>
                <li
                  class="skill"
                  style="
                    line-height: 24px;
                    font-size: 16px;
                    list-style: disc;
                    color: black;
                  "
                >
                  <%=certifications[i]%>
                </li>
                <% } %>
              </ul>
            </div>
          </div>
        </div>
        <% } %>

        <!--// .yui-gf -->
        <% if (additionalDetails && additionalDetails.length>0) { %>

        <div
          class="yui-gf"
          style="margin-bottom: 1em; border-bottom: 1px solid #ccc"
        >
          <div class="yui-u first">
            <h2 style="font-size: 152%; font-weight: bold; color: #333">
              Additional Details
            </h2>
          </div>
          <div class="yui-u" style="padding-right: 10px">
            <% for( let i = 0; i < additionalDetails.length; i++ ) { %>

            <div
              class="job"
              style="
                position: relative;
                padding-left: 50px;
                margin-bottom: 1em;
                padding-bottom: 1em;
              "
            >
              <h2 style="font-size: 152%; line-height: 24px; color: #333">
                <%= additionalDetails[i].key%> : <%=additionalDetails[i].value%>
              </h2>
            </div>
            <% } %>
          </div>
        </div>
        <% } %>

        <!--// .yui-gf -->
        <% if (refrences && refrences.length>0) { %>

        <div
          class="yui-gf"
          style="margin-bottom: 5px; border-bottom: 1px solid #ccc"
        >
          <div class="yui-u first">
            <h2 style="font-size: 152%; font-weight: bold; color: #333">
              References
            </h2>
          </div>
          <div class="yui-u" style="padding-right: 10px">
            <% for( var i=0; i < refrences.length; i++) { %>

            <div
              class="job"
              style="
                position: relative;
                padding-left: 50px;
                margin-bottom: 1em;
                padding-bottom: 1em;
              "
            >
              <h2 style="font-size: 152%; line-height: 24px; color: #333">
                <%=refrences[i].name%>
              </h2>
              <h3 style="font-size: 122%; line-height: 24px; color: #333">
                <%=refrences[i].position%>
              </h3>
              <h4 style="font-size: 122%; line-height: 24px; color: #333">
                <%=refrences[i].phone%>
              </h4>
              <h4 style="font-size: 122%; line-height: 24px; color: #333">
                <%=refrences[i].email%>
              </h4>
            </div>
            <% } %>
          </div>
        </div>
        <% } %>
      </div>
    </div>
  </body>
</html>
